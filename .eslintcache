gies/Security-Project/client-app/src/features/form/SignInForm.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/customer/Account.tsx",["175","176","177"],"import React, { useContext, useEffect, useState } from \"react\";\nimport { Grid, Button, Card, Image, Icon } from \"semantic-ui-react\";\nimport { RootStoreContext } from \"../../App/stores/RootStore\";\nimport { UserFormValues } from \"../../App/models/user\";\nimport AddressForm from \"../form/AddressForm\";\nimport { EditProfile } from \"../form/EditProfile\";\nimport { AddressFormValues } from \"../../App/models/address\";\nimport AddressPresentation from \"../form/AddressPresentation\";\nimport { RouteComponentProps } from \"react-router-dom\";\nimport UserAdsCatalog from \"./UserAdsCatalog\";\nimport { history } from \"../..\";\n\nconst Account: React.FC<RouteComponentProps> = () => {\n  const rootStore = useContext(RootStoreContext);\n  const { loadUser } = rootStore.userStore;\n  const { token } = rootStore.commonStore;\n  const { loadAddress } = rootStore.addressStore;\n  const [user, setUser] = useState(new UserFormValues());\n  const [address, setAddress] = useState(new AddressFormValues());\n\n  const Status = {\n    editProfile: \"EditProfile\",\n    addressForm: \"addressForm\",\n    adsForm: \"adsForm\",\n    postForm: \"postForm\",\n    userAddress: \"userAddress\",\n  };\n\n  const [status, setStatus] = useState(Status.adsForm);\n\n  useEffect(() => {\n    if (token) {\n      loadUser().then((user) => setUser(new UserFormValues(user)));\n    }\n  }, [loadUser, token, setUser]);\n\n  useEffect(() => {\n    if (user) {\n      loadAddress().then((address) =>\n        setAddress(new AddressFormValues(address))\n      );\n    }\n  }, [loadAddress, user, setAddress]);\n\n  const getBody = () => {\n    switch (status) {\n      case Status.adsForm:\n        return <UserAdsCatalog />;\n      case Status.editProfile:\n        return (\n          <div className=\"center\">\n            <EditProfile />\n          </div>\n        );\n      case Status.addressForm:\n        return (\n          <div className=\"center\">\n            <AddressForm />\n          </div>\n        );\n      case Status.userAddress:\n        return (\n          <div className=\"center\">\n            <AddressPresentation />\n            <div style={{ textAlign: \"center\" }}>\n              <Button\n                onClick={() => setStatus(Status.addressForm)}\n                content=\"Modify address\"\n                color=\"yellow\"\n              ></Button>\n            </div>\n          </div>\n        );\n      case Status.postForm:\n        return (\n          <div className=\"center\" style={{ textAlign: \"center\" }}>\n            <p>Messages Form</p>\n          </div>\n        );\n    }\n  };\n\n  return (\n    <Grid style={{ marginLeft: \"2%\" }}>\n      <Grid.Row>\n        <Grid.Column width={4}>\n          <Card color=\"yellow\" fluid>\n            <Image src=\"unnamed.png\" centered fluid />\n            <Card.Content>\n              <Card.Header>{user.firstName + \" \" + user.lastName}</Card.Header>\n              <Card.Description>\n                <Icon name=\"edit\" />\n                <a onClick={() => setStatus(Status.editProfile)}>\n                  Edit profile\n                </a>\n              </Card.Description>\n              <Card.Description>\n                <Icon name=\"home\" />\n                {address.street ? (\n                  <a onClick={() => setStatus(Status.userAddress)}>\n                    My Address\n                  </a>\n                ) : (\n                  <a onClick={() => setStatus(Status.addressForm)}>\n                    Add Address\n                  </a>\n                )}\n              </Card.Description>\n            </Card.Content>\n          </Card>\n          <Button.Group basic color=\"yellow\" vertical fluid>\n            <Button onClick={() => setStatus(Status.adsForm)}>My Ads</Button>\n            <Button onClick={() => history.push(\"/ads/create\")}>\n              Post New Ads\n            </Button>\n          </Button.Group>\n        </Grid.Column>\n        <Grid.Column width={12}>{getBody()}</Grid.Column>\n      </Grid.Row>\n    </Grid>\n  );\n};\n\nexport default Account;\n","/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/customer/RegisterSuccess.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/customer/VerifyEmail.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/footer/AppFooter.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/nav/NavBar.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/home/HomePage.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/ads/AdsShowcase.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/App/stores/CommonStore.ts",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/App/stores/UserStore.ts",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/App/stores/AddressStore.ts",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/App/stores/AdStore.ts",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/App/models/user.ts",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/App/models/address.ts",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/form/ErrorMessage.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/App/api/Agent.ts",["178"],"import axios from \"axios\";\nimport { IAddress } from \"../models/address\";\nimport { IAds } from \"../models/ads\";\nimport { IUser, IUserFormValues } from \"../models/user\";\n\n// axios.defaults.baseURL = \"https://kijaja.me/api\";\naxios.defaults.baseURL = \"http://localhost:5000/api\";\n\naxios.interceptors.request.use(\n  (config) => {\n    const token = window.localStorage.getItem(\"jwt\");\n    if (token) {\n      config.headers.Authorization = `Bearer ${token}`;\n    }\n    return config;\n  },\n  (error) => {\n    return Promise.reject(error);\n  }\n);\n\naxios.interceptors.response.use(undefined, (error) => {\n  throw error.response;\n});\n\nconst Users = {\n  current: (): Promise<IUser> =>\n    axios.get(\"/user\").then((response) => response.data),\n\n  login: (user: IUserFormValues): Promise<IUser> =>\n    axios.post(`/user/login`, user).then((response) => response.data),\n\n  registerUser: (user: IUserFormValues): Promise<void> =>\n    axios.post(`/user/register`, user).then((response) => response.data),\n\n  verifyEmail: (token: string, email: string): Promise<void> =>\n    axios.post(`/user/verifyEmail`, { token, email }),\n\n  resendEmailVerification: (email: string): Promise<void> =>\n    axios.get(`/user/resendEmailVerification?email=${email}`),\n\n  getMyAds: (): Promise<IAds[]> =>\n    axios.get(\"/user/myAds\").then((response) => response.data),\n\n  edit: (values: IUser): Promise<void> =>\n    axios.post(`/user/edit`, values).then((response) => response.data),\n};\n\nconst Address = {\n  create: (values: IAddress): Promise<void> =>\n    axios.post(\"/address/\", values).then((response) => response.data),\n\n  loadAddress: (): Promise<IAddress> =>\n    axios.get(\"/address\").then((response) => response.data),\n\n  modify: (values: IAddress): Promise<void> =>\n    axios.post(\"/address/edit\", values).then((response) => response.data),\n\n  delete: (): Promise<void> =>\n    axios.post(\"/address/delete\").then((response) => response.data),\n};\n\nconst Ads = {\n  create: (values: IAds): Promise<void> =>\n    axios.post(\"/ads\", values).then((response) => response.data),\n\n  modify: (id: string, values: IAds): Promise<void> =>\n    axios.put(`/ads/${id}`, values).then((response) => response.data),\n\n  delete: (adId: string): Promise<void> =>\n    axios.delete(\"/ads\", {data: {id: adId}}).then((response) => response.data),\n\n  List: (): Promise<IAds[]> =>\n    axios.get(\"/ads\").then((response) => response.data),\n\n  Details: (id: string): Promise<IAds> =>\n    axios.get(`/ads/${id}`).then((response) => response.data),\n};\n\nexport default {\n  Users,\n  Address,\n  Ads,\n};\n","/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/form/AddressForm.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/form/AddressPresentation.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/form/EditProfile.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/App/common/form/TextInputIcon.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/App/common/form/TextInput.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/App/common/loading/LoadingComponent.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/ads/AdsDetailsPage.tsx",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/App/models/ads.ts",[],"/Users/antoinehebert/Desktop/Emerging Technologies/Security-Project/client-app/src/features/customer/UserAdsCatalog.tsx",["179","180"],"import { observer } from \"mobx-react-lite\";\nimport React, { useContext, useEffect, useState } from \"react\";\nimport {\n  Button,\n  Card,\n  Container,\n  Grid,\n  Header,\n  Icon,\n  Image,\n  Label,\n  Segment,\n} from \"semantic-ui-react\";\nimport { history } from \"../..\";\nimport { LoadingComponent } from \"../..